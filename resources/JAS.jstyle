incollection=\format[Authors(LastFirst,InitialsNoSpace,LastSep= , )]{\author}, \year\uniq. \title. In \format[Authors(LastFirst,InitialsNoSpace,LastSep= , )]{\editor} (\format[IfPlural(Eds.,Ed.)]{\editor}), \title\begin{edition} (\edition ed.)\end{edition}\begin{volume} (Vol. \volume)\end{volume} \begin{pages}(pp. \end{pages}\format[FormatPagesForHTML]{\pages}\begin{pages})\end{pages}.\begin{publisher} \publisher,\end{publisher}\begin{address} \address\end{address}.

default=\format[Authors(LastFirst,InitialsNoSpace,LastSep= , )]{\author}, \year\uniq. \title. \note. \begin{doi}doi:\format[DOIStrip]{\doi}\end{doi}
