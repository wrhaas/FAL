article=\format[Authors(LastFirst,InitialsNoSpace,LastSep= , )]{\author}, \year\uniq. \title. \journal \volume\begin{pages}, \end{pages}\format[FormatPagesForHTML]{\pages}. \begin{doi}doi:\format[DOIStrip]{\doi}\end{doi}

